{
  "name": "@nuxt-awesome/theme",
  "version": "2.0.1",
  "description": "a Nuxt 3 starter template & nuxt layer theme with a lot of useful features, integrated with TailwindCSS 3",
  "author": "viandwi24",
  "license": "MIT",
  "type": "module",
  "main": "./nuxt.config.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/viandwi24/nuxt3-awesome-starter"
  },
  "files": [
    "assets",
    "components",
    "composables",
    "layouts",
    "modules",
    "pages",
    "public",
    "stores",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts",
    "nuxt.schema.ts",
    "tailwind.config.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "nuxt build ./app",
    "dev": "nuxt dev ./app --no-clear",
    "generate": "nuxt generate ./app",
    "preview": "nuxt preview ./app",
    "postinstall": "nuxt prepare ./app",
    "dev:starter": "nuxt dev ./examples/starter",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "release": "release-it"
  },
  "devDependencies": {
    "@nuxt/content": "^2.5.2",
    "@nuxt/devtools": "^0.2.5",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^4.0.2",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@nuxtjs/tailwindcss": "^6.6.0",
    "@pinia/nuxt": "^0.4.7",
    "consola": "^3.0.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "^3.4.1",
    "nuxt-headlessui": "^1.1.1",
    "nuxt-icon": "^0.3.3",
    "nuxt-windicss": "^2.6.0",
    "pinia": "^2.0.33",
    "prettier": "^2.8.7",
    "release-it": "^15.9.0",
    "sass": "^1.59.3",
    "serve": "^14.2.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.4",
    "vue-tsc": "1.0.13"
  },
  "release-it": {
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  },
  "dependencies": {
    "@nuxt/image": "^0.7.1",
    "@nuxt/types": "^2.16.3"
  }
}
